{
    "Job": {
        "Region": "global",
        "ID": "{{id}}",
        "Name": "{{id}}",
        "Type": "service",
        "Priority": 50,
        "AllAtOnce": false,
        "Datacenters": [
            "dc1"
        ],
        "Constraints": null,
        "TaskGroups": [
            {
                "Name": "zeppelin",
                "Count": 1,
                "Constraints": null,
                "Tasks": [
                    {
                        "Name": "zeppelin",
                        "Driver": "raw_exec",
                        "User": "",
                        "Config": {
                            "args": [
                                "-p",
                                "8080:8080",
                                "-v",
                                "${NOMAD_TASK_DIR}/notebooks:/usr/local/zeppelin/notebooks",
                                "frosner/zeppelin"
                            ],
                            "command": "nomad-docker-wrapper"
                        },
                        "Constraints": null,
                        "Env": {
                            "NOMAD_DOCKER_CONTAINER_NAME": "{{id}}"
                        },
                        "Services": [
                            {
                                "Id": "",
                                "Name": "{{id}}-ui",
                                "Tags": null,
                                "PortLabel": "ui",
                                "Checks": null
                            }
                        ],
                        "Resources": {
                            "CPU": 2000,
                            "MemoryMB": 1024,
                            "DiskMB": 300,
                            "IOPS": 0,
                            "Networks": [
                                {
                                    "Public": false,
                                    "CIDR": "",
                                    "ReservedPorts": [
                                        {
                                            "Label": "ui",
                                            "Value": 8080
                                        }
                                    ],
                                    "DynamicPorts": null,
                                    "IP": "",
                                    "MBits": 10
                                }
                            ]
                        },
                        "Meta": null,
                        "KillTimeout": 5000000000,
                        "LogConfig": {
                            "MaxFiles": 10,
                            "MaxFileSizeMB": 10
                        },
                        "Artifacts": [
                            {
                                "GetterSource": "https://raw.githubusercontent.com/FRosner/nomad-docker-wrapper/1.0.0/nomad-docker-wrapper",
                                "GetterOptions": {},
                                "RelativeDest": "local/"
                            }
                        ]
                    }
                ],
                "RestartPolicy": {
                    "Interval": 60000000000,
                    "Attempts": 2,
                    "Delay": 15000000000,
                    "Mode": "delay"
                },
                "Meta": null
            }
        ],
        "Update": {
            "Stagger": 0,
            "MaxParallel": 0
        },
        "Periodic": null,
        "Meta": null,
        "Status": "",
        "StatusDescription": "",
        "CreateIndex": 0,
        "ModifyIndex": 0
    }
}
